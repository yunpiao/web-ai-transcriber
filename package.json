{
  "name": "smart-search-extension",
  "version": "4.9.2",
  "description": "Chrome extension for AI-powered web content transcription with browsing history tracking",
  "scripts": {
    "test": "npm run test:core",
    "test:all": "npm run test:core && npm run test:e2e",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "test:e2e:headed": "HEADLESS=false jest --config=jest.e2e.config.js",
    "test:e2e:debug": "HEADLESS=false DEBUG=true jest --config=jest.e2e.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:ci:all": "npm run test:core && npm run test:e2e",
    "test:core": "npm run test:unit && npm run test:integration",
    "version": "node -e \"const fs=require('fs');const p='./smart-search-extension/manifest.json';const m=JSON.parse(fs.readFileSync(p));m.version=require('./package.json').version;fs.writeFileSync(p,JSON.stringify(m,null,2)+'\\n')\" && git add smart-search-extension/manifest.json",
    "postversion": "git push && git push origin v$npm_package_version"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/jest": "^29.5.12",
    "fake-indexeddb": "^5.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "puppeteer": "^24.25.0",
    "ws": "^8.18.3"
  },
  "keywords": [
    "chrome-extension",
    "ai",
    "transcription",
    "browsing-history"
  ],
  "author": "",
  "license": "MIT"
}
